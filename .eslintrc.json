{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": "off",
    "import/no-extraneous-dependencies": "off",
    "linebreak-style": "off",
    "indent": "off",
    "eol-last": "off"
  }
}
